{
  "manifest_version": 3,
  "name": "NTP Redirect",
  "version": "1.0",
  "description": "Redirects the New Tab Page to a custom URL.",
  "permissions": ["storage"],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "options.html"
}
